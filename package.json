  "author": "Icalia Labs",
  "contributors": [
    {
      "name": "Vov",
      "url": "https://ical.ink/vov"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "aws-sdk": "^2.1100.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4",
    "yaml": "^1.10.2"
  }
